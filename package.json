{
  "name": "@gomah/prisma-serverless",
  "version": "1.0.0",
  "repository": "git@github.com:gomah/prisma-serverless.git",
  "author": "Gomah <hi@gomah.fr>",
  "license": "MIT",
  "private": true,
  "scripts": {
    "build": "sls package",
    "nexus-generate": "npx nexus-prisma-generate --client src/generated/prisma-client --output src/generated/nexus-prisma",
    "dev": "npm-run-all --parallel dev:server dev:playground",
    "dev:server": "sls offline --dontPrintOutput --stage dev",
    "dev:playground": "graphql playground --port 5000",
    "deploy:dev": "yarn build && sls deploy --stage development",
    "deploy:prod": "yarn build && sls deploy --stage production",
    "test:lint": "eslint --ext .js,.ts,.gql,.graphql src/",
    "test": "yarn test:lint",
    "prisma": "prisma",
    "prisma:admin": "prisma admin"
  },
  "pre-commit": [
    "test:lint"
  ],
  "resolutions": {
    "graphql": "14.5.8"
  },
  "dependencies": {
    "apollo-server-lambda": "^2.9.15",
    "aws-sdk": "2.485.0",
    "bcryptjs": "^2.4.3",
    "dotenv": "^8.2.0",
    "graphql": "^14.5.8",
    "graphql-import": "^0.7.1",
    "graphql-middleware": "^4.0.2",
    "graphql-shield": "^7.0.7",
    "graphql-tools": "^4.0.6",
    "jsonwebtoken": "^8.5.1",
    "nexus": "^0.11.7",
    "nexus-prisma": "0.3.8",
    "prisma-client-lib": "^1.34.10"
  },
  "devDependencies": {
    "@babel/core": "^7.7.7",
    "@babel/node": "^7.7.7",
    "@babel/preset-env": "^7.7.7",
    "@hapi/address": "^3.2.2",
    "@types/aws-lambda": "^8.10.39",
    "@types/bcryptjs": "^2.4.2",
    "@types/dotenv": "^8.2.0",
    "@types/jsonwebtoken": "^8.3.5",
    "@types/node": "^13.1.2",
    "@types/ws": "^6.0.4",
    "@typescript-eslint/eslint-plugin": "^2.15.0",
    "@typescript-eslint/parser": "^2.14.0",
    "aws-lambda": "^1.0.4",
    "babel-loader": "^8.0.6",
    "babel-plugin-module-resolver": "^4.0.0",
    "copy-webpack-plugin": "^5.1.1",
    "cross-env": "^6.0.3",
    "eslint": "^6.8.0",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-config-prettier": "^6.9.0",
    "eslint-import-resolver-webpack": "^0.12.0",
    "eslint-plugin-graphql": "^3.1.1",
    "eslint-plugin-import": "^2.19.1",
    "eslint-plugin-prettier": "^3.1.2",
    "graphql-cli": "^3.0.14",
    "graphql-import-loader": "^0.2.1",
    "npm-run-all": "^4.1.5",
    "pre-commit": "^1.2.2",
    "prettier": "^1.19.1",
    "prisma": "^1.34.10",
    "serverless": "^1.60.4",
    "serverless-dotenv-plugin": "^2.1.1",
    "serverless-offline": "^5.12.1",
    "serverless-prune-plugin": "^1.4.1",
    "serverless-webpack": "^5.3.1",
    "ts-loader": "^6.2.1",
    "ts-node": "^8.5.4",
    "ts-node-dev": "^1.0.0-pre.44",
    "typescript": "3.4.5",
    "webpack": "^4.41.5",
    "webpack-cli": "^3.3.10",
    "webpack-node-externals": "^1.7.2"
  }
}
