{
  "name": "@gomah/prisma-serverless",
  "version": "1.0.0",
  "repository": "git@github.com:gomah/prisma-serverless.git",
  "author": "Gomah <hi@gomah.fr>",
  "license": "UNLICENSED",
  "private": true,
  "scripts": {
    "build": "rm -rf build/ && yarn nexus-generate && tsc -b",
    "nexus-generate": "npx nexus-prisma-generate --client src/generated/prisma-client --output src/generated/nexus-prisma",
    "dev": "npm-run-all --parallel dev:server dev:playground",
    "dev:server": "sls offline --dontPrintOutput --stage dev",
    "dev:playground": "graphql playground --port 5000",
    "deploy:dev": "yarn build && sls deploy --stage dev",
    "test:lint": "eslint --ext .js,.ts,.gql,.graphql src/ prisma/",
    "prisma": "prisma"
  },
  "pre-commit": [
    "test:lint"
  ],
  "resolutions": {
    "graphql": "14.1.1"
  },
  "dependencies": {
    "apollo-server-lambda": "^2.4.2",
    "axios": "^0.18.0",
    "bcryptjs": "^2.4.3",
    "compression": "^1.7.3",
